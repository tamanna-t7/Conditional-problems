#include<stdio.h>
int main()
{
    int n;
    printf("please enter your marks:");
    scanf("%d",&n);
    if(n>=80&&n<=100)
        printf("marks=A+",n);
    else if(n>=70&&n<=79)
        printf("marks=A",n);
    else if(n>=60&&n<=69)
        printf("marks=A-",n);
    else if(n>=50&&n<=59)
        printf("marks=B",n);
    else if(n>=40&&n<=49)
        printf("marks=C",n);
    else if(n>=30&&n<=39)
        printf("marks=D",n);
    else
        printf("marks=F",n);
        return 0;
}
